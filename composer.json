{
    "name": "tg-bot-api/bot-api-routing",
    "type": "library",
    "description": "it's routing package for tg-bot-api/bot-api-base",
    "keywords": [
        "tg-bot-api",
        "bot-api-routing"
    ],
    "homepage": "https://github.com/tg-bot-api/bot-api-routing",
    "license": "MIT",
    "authors": [
        {
            "name": "greenplugin",
            "email": "wformps@gmail.com",
            "homepage": "https://github.com/greenplugin",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "tg-bot-api/bot-api-base": "^1",
      "ext-mbstring": "*",
      "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "TgBotApi\\BotApiRouting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TgBotApi\\BotApiRouting\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
